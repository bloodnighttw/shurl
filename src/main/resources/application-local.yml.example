spring:
  application:
    name: shurl
  datasource:
    url: ${DB_URL}
    username: ${DB_USERNAME}
    password: ${DB_PASSWORD}
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate:
        format_sql: true

    database: postgresql
  security:
    user:
      password: ${SECURITY_PASSWORD}

shuri:
  api-prefix: api